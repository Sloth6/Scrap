-var content = JSON.parse(element.content)
-var url = decodeURIComponent(content.url)
.card.website
  a(href=url, target='_blank')
    .card.image
      img(src=content.image)
    .card.header.text
      .title
        p!=content.title
      .url
        p!=content.domain
      .description
        p!=content.description
  .ui-resizable-handle.ui-resizable-se
