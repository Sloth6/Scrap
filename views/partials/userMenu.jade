- var creator = current_space.creator
- var isCreator = current_user.id == creator.id

script.
  creator="#{creator.name}";
  username = "#{current_user.name}";
  userId = "#{current_user.id}";
  users =  !{JSON.stringify(users)}
  names = !{JSON.stringify(names)}

ul.users
  if isCreator
    li.user.color0.online(data-id=current_user.id)
      span.owner #{current_user.name}
  else
    li John Appleseed
    li.user(data-id=current_user.id)=current_user.name
  each user in current_space.users
    if user.id != current_user.id
      if user.id != creator.id && isCreator
        li.deletable-user(data-id=user.id)=user.name
      else if user.id == creator.id
        li(data-id=user.id) #{user.name} (owner)
      else
        li(data-id=user.id)=user.name
  li.user.color4.online Denice Lavigne
  li.user.color5.online Idell Morace
  li.user.color5 Felisha Kilday
  li.user.color6 Concetta Morehouse
  li.user.color7 Maegan Hochstetler
  li.user.color8.online Thomas Pera
  li.user.color9 Bryan Velarde
  li.settings
    ul.menu.settings
      li settings
      li
        form.add-user
          input(type="text", name="user[email]", placeholder="Invite someone to this space")
          input(type="submit", style="visibility: hidden;")
      li
        a.logout(href="/logout") Logout